<table class='table'>
  <thead>
    <tr>
      <th>画像</th>
      <th>UserName</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td>
        <%= link_to user_path(user.id) do %>
          <%= image_tag user.get_profile_image, class: 'rounded-circle', size: '50x50' %>
          </td>
          <td>
          <%= user.name %>
          </td>
        <% end %>
        <td><%#= link_to 'Show', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>
